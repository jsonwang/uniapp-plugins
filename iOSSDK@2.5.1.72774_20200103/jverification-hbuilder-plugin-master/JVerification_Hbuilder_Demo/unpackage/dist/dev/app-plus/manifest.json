{
  "@platforms": [
    "android",
    "iPhone",
    "iPad"
  ],
  "id": "__UNI__AB1AADD",
  "name": "JVerification_Hbuilder_Demo",
  "version": {
    "name": "1.0.0",
    "code": "100"
